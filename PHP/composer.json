{
    "name": "codecop/cook-more-refactoring",
    "type": "project",
    "license": "BSD",
    "require": {
        "php": ">=7.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.6.*",
        "mockery/mockery": "1.6.*"
    },
    "autoload": {
        "psr-4": {
            "Minimal\\": "src\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests\/"
        }
    },
    "scripts": {
        "start": "php ./src/Client.php",
        "test": "phpunit",
        "tests": "phpunit"
    },
    "config": {
        "optimize-autoloader": true
    }
}
